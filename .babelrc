{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@features": "./features",
          "@pages": "./pages",
          "@shared": "./shared",
          "@icons": "./public/img",
          "@styles": "./styles"
        }
      }
    ]
  ]
}
